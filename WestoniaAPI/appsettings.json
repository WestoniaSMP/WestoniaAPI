{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.DataProtection": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WestoniaDb": "Server={ServerHostnameOrIP};Database={DatabaseName};User Id={UserId};Password={Password};Encrypt=True;TrustServerCertificate=True" // must be used for debugging
    // "WestoniaDb": "Server=localhost;Database={DatabaseName};User Id={UserId};Password={Password};Encrypt=True;TrustServerCertificate=True" // Must be used for ef commands
  },
  "DiscordAuth": {
    "RedirectUri": "/api/auth/discordAuthCallback",
    "Scopes": "identify,email", // Comma-separated list of scopes
    "ClientId": "{InsertClientIdHere}",
    "ClientSecret": "{InsertClientSecretHere}"
  },
  "JwtSettings": {
    "Issuer": "https://localhost:8081/",
    "Audience": "https://localhost:8081/",
    "EncryptionKey": "{InsertEncryptionKeyHere}"
  }
}